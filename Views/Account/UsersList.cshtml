@using WebApp1.Models
@model IEnumerable<ApplicationUser>

@{
    ViewData["Title"] = "Users List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Users List</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Email</th>
            <th>City</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Email</td>
                <td>@user.City</td>
            </tr>
        }
    </tbody>
</table>